pipeline {

    agent {
        node {
            label 'jenkins-slave'
        }
    }
   
    stages {
         stage('Deploy app') {
         steps{
            build job: 'childDeployJob',
            parameters: [
                string(name: 'STAND', value: "Stage")
                string(name: 'TAG', value: "Some tag")
                string(name: 'NAMESPACE', value: "work-app")
                string(name: 'DEPLOYNAME', value: "Develop")
            ]
         }
         }
    }
}

